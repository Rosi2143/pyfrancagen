{
   "version": "2.0.0",
   "tasks": [
      {
         "type": "cppbuild",
         "label": "C/C++: g++ build active file",
         "command": "/usr/bin/g++",
         "args": [
            "-fdiagnostics-color=always",
            "-I${fileDirname}",
            "-I${workspaceFolder}/cpp",
            "-I${workspaceFolder}/lbs/src_gen",
            "-g",
            "${workspaceFolder}/cpp/main_test.cpp",
            "${workspaceFolder}/lbs/src_gen/test.cpp",
            "${workspaceFolder}/cpp/serializer.cpp",
            "-o",
            "${workspaceFolder}/cpp/serializer"
         ],
         "options": {
            "cwd": "${fileDirname}"
         },
         "problemMatcher": [
            "$gcc"
         ],
         "group": {
            "kind": "build",
            "isDefault": true
         },
         "detail": "Task generated by Debugger."
      },
      {
         "type": "cppbuild",
         "label": "C/C++: g++ build active test",
         "command": "/usr/bin/g++",
         "args": [
            "-fdiagnostics-color=always",
            "-I${fileDirname}",
            "-I${workspaceFolder}/cpp",
            "-I${workspaceFolder}/lbs/src_gen",
            "-g",
            "${workspaceFolder}/cpp/utest_test.cpp",
            "${workspaceFolder}/lbs/src_gen/test.cpp",
            "${workspaceFolder}/cpp/serializer.cpp",
            "-lgtest",
            "-lgtest_main",
            "-lpthread",
            "-o",
            "${workspaceFolder}/cpp/utest_test"
         ],
         "options": {
            "cwd": "${fileDirname}"
         },
         "problemMatcher": [
            "$gcc"
         ],
         "group": {
            "kind": "build",
            "isDefault": true
         },
         "detail": "Task generated by Debugger."
      }
   ]
}